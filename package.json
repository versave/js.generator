{
  "name": "js.generator",
  "version": "1.0.0",
  "description": "JS file generator with selected methods",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
     "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/css/images/icons/mac/icon.icns --prune=true --out=release-builds",
      "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/css/images/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"JS Generator\"",    
      "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/css/images/icons/png/icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/versave/js.generator"
  },
  "author": "Vasil Zhivkov",
  "license": "ISC",
  "dependencies": {
    "electron": "^4.1.1",
    "js-beautify": "^1.9.1"
  },
  "devDependencies": {
    "electron-packager": "^13.1.1"
  }
}
